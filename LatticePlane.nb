       RowBox[{
        RowBox[{
         RowBox[{"ToStandardSetting", "[", 
          RowBox[{"pg", ",", "#"}], "]"}], "&"}], "/@", "\[ScriptR]"}], 
       "]"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{"degenerate", " ", "sets"}], "*)"}], "\[IndentingNewLine]", 
